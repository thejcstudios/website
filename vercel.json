{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "thejcstudios.com"
        }
      ],
      "destination": "https://www.thejcstudios.com",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "thejcstudios.vercel.app"
        }
      ],
      "destination": "https://www.thejcstudios.com",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-src 'self' https://www.youtube.com https://www.youtube-nocookie.com https://drive.google.com https://docs.google.com https://apis.google.com;"
        }
      ]
    }
  ]
}
